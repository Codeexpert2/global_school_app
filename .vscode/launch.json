{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "flutter_template",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "flutter_template (debug)",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define=ENV=dev"
            ]
        },
        {
            "name": "flutter_template (with env vars)",
            "request": "launch",
            "type": "dart",
            "env": {
                "API_BASE_URL": "https://dev.api.example.com",
                "DEBUG_MODE": "true"
            }
        },
        {
            "name": "flutter_template (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "flutter_template (profile memory)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": [
                "--trace-skia"
            ]
        },
        {
            "name": "flutter_template (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "flutter_template (with pre-launch task)",
            "request": "launch",
            "type": "dart",
            "preLaunchTask": "build-flavor-dev"
        }
    ]
}